port 80;

root /usr/src/projects/iceberg-webserver/;

handler echo {
  location /echo;
}

handler static {
  location /static1;
  root public/file1;
}

handler static {
  location /static2;
  root public/file2;
}

handler status {
  location /status;
}

handler proxy {
  location /ucla;
  root http://www.ucla.edu;
}

handler new_meme {
  location /meme/create;
}

handler view_meme {
  location /meme/view;
}
